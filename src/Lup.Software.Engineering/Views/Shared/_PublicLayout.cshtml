<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="Content-Language" content="en">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - L端p</title>
    <link rel="stylesheet" href="https://content.lup.com.au/css/login.min.css" />
    <link rel="shortcut icon" href="https://content.lup.com.au/images/lup-icon-35.png">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="description" content="Lup Events - An error has occured.">
    <meta name="author" content="L端p Engineers \\(^_^)\\ //(^_^)//">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <script src="https://code.jquery.com/jquery-2.2.4.min.js"
            asp-fallback-src="~/lib/jquery/dist/jquery.min.js" asp-fallback-test="window.jQuery"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" integrity="sha384-0mSbJDEHialfmuBBQP6A4Qrprq5OVfW37PRR3j5ELqxss1yVqOtnepnHVP9aJ7xS" crossorigin="anonymous"
            asp-fallback-src="~/lib/bootstrap-sass/assets/javascripts/bootstrap.min.js" asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"></script>
</head>
<body class="login-page">
    <div class="container">
        <main id="content" class="widget-login-container" role="main">
            <div class="row">
                <div class="col-sm-12 col-lg-6 col-lg-offset-3 col-md-8 col-md-offset-2">
                    <h4 class="widget-login-logo">
                        <img src="https://content.lup.com.au/images/logo-black-150.png" class="img-responsive" alt="L端p" />
                    </h4>
                    <section class="widget widget-login">
                        @RenderBody()
                        @RenderSection("WidgetFooter", false)
                    </section>
                </div>
            </div>
        </main>
        <footer class="page-footer">
            2016 &copy; L端p
            <div>
                @env.EnvironmentName @Environment.MachineName @PlatformServices.Default.Application.ApplicationVersion
            </div>
        </footer>
    </div>

    <!-- Latest compiled and minified JavaScript -->
    <environment names="Development">
        <script src="https://content.lup.com.au/scripts/settings.js" asp-append-version="true"></script>
        <script src="https://content.lup.com.au/scripts/application.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://content.lup.com.au/scripts/application.min.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("Scripts", required: false)
</body>
</html>